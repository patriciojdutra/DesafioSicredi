<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.patricio.dutra.desafiosicredi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@drawable/sicredi"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sicredi"
        android:supportsRtl="true"
        android:theme="@style/Theme.DesafioSicredi">
        <activity android:name=".view.DetalheEventoActivity"></activity>
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>